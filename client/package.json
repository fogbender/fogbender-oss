    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@headlessui/react": "^1.7.4",
    "@hyperdx/browser": "^0.19.3",
    "@reach/dialog": "^0.18.0",
    "@reduxjs/toolkit": "^1.9.7",
    "@simbathesailor/use-what-changed": "^2.0.0",
    "@tanstack/react-virtual": "^3.0.0-beta.18",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "@total-typescript/ts-reset": "^0.3.7",
    "@types/classnames": "^2.2.10",
    "@types/jest": "25.2.1",
    "@types/node": "13.13.2",
    "@types/react": "^17.0.52",
    "@types/react-dom": "^17.0.18",
    "@types/react-grid-layout": "^1.1.1",
    "@types/react-redux": "^7.1.7",
    "@types/throttle-debounce": "^5.0.2",
    "@types/uuid": "^7.0.3",
    "astro": "^4.3.2",
    "autoprefixer": "^10.4.12",
    "chrono-node": "^2.4.1",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.8.35",
    "emoji-picker-react": "^4.5.2",
    "fogbender": "*",
    "fogbender-proto": "*",
    "fuzzy": "^0.1.3",
    "highlight.js": "^11.4.0",
    "jotai": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "postcss": "8.4.18",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.1.2",
    "pretty-bytes": "^6.0.0",
    "qgp": "*",
    "query-string": "^6.12.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-error-boundary": "^2.2.1",
    "react-grid-layout": "^1.4.3",
    "react-redux": "^7.2.0",
    "react-responsive-carousel": "^3.2.10",
    "react-scripts": "^4.0.0",
    "react-textarea-autosize": "^8.5.3",
    "redux": "4.2.1",
    "rehype": "^12.0.1",
    "rehype-highlight": "^6.0.0",
    "throttle-debounce": "^5.0.0",
    "typescript": "^5.2.2",
    "uuid": "^7.0.3"
  },
  "scripts": {
    "start": "npm-run-all -p watch:loader watch:js",
    "dev2": "vite --config qgp.config.mjs",
    "astro": "astro",
    "dev": "run-s start",
    "build-client": "cd ..; yarn build-client",
    "build": "NODE_ENV=production npm-run-all build:loader build:js",
    "lint:full": "npm-run-all -p lint:tsc lint:astro lint",
    "lint:tsc": "tsc --noEmit",
    "lint:astro": "astro check",
    "lint": "tslint --project .",
    "build:loader": "webpack --config loader.webpack.config.js",
    "watch:loader": "webpack --config loader.webpack.config.js --watch",
    "build:snippet": "tsc --skipLibCheck true src/snippet.full.ts --outDir ./public/ && (cat public/snippet.full.js | tail -n +3 > public/snippet.js) && (terser public/snippet.js --compress --mangle --output public/snippet.min.js) && wc -c public/snippet.min.js && git grep -q -F \"$(cat public/snippet.min.js)\" ../storefront",
    "watch:js": "astro dev",
    "build:js": "astro check && tsc --noEmit && astro build",
    "test": "react-scripts test",
    "preview": "astro preview"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^2.2.0",
    "css-element-queries": "^1.2.3",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.5.0",
    "tslint": "6.1.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "vite-plugin-checker": "^0.6.2",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
