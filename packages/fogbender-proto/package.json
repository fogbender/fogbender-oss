{
  "name": "fogbender-proto",
  "version": "0.17.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Made with create-react-library",
  "author": "Fogbender Software, Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogbender/fogbender-oss/blob/main/packages/fogbender-proto"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc --version && tsup --dts",
    "start": "run-p build start:tsc",
    "start:tsup": "tsup --watch",
    "start:tsc": "tsc --noEmit --watch | awk '{gsub(/\\033c/,\"\") system(\"\")}1'",
    "prepare": "run-s build",
    "test": "run-s test:lint",
    "test:lint": "eslint . || true",
    "predeploy": "cd example && yarn install && yarn run build"
  },
  "peerDependencies": {
    "jotai": ">=0.7.2",
    "react": ">=16.0.0"
  },
  "dependencies": {
    "@types/bson": "^4.2.0",
    "bson": "4.7.2",
    "immer": "^7.0.9",
    "lodash.throttle": "^4.1.1",
    "react-use-websocket": "^4.7.0",
    "use-immer": "^0.4.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/jest": "^25.1.4",
    "@types/lodash.throttle": "^4.1.7",
    "@types/node": "13.13.2",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "globals": "^16.0.0",
    "jotai": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "files": [
    "dist"
  ]
}
