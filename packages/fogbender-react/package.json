{
  "name": "fogbender-react",
  "version": "0.5.2",
  "publishConfig": {
    "access": "public"
  },
  "description": "Made with create-react-library",
  "author": "Fogbender Software, Inc.",
  "license": "MIT",
  "main": "dist/index.cjs",
  "type": "module",
  "module": "dist/index.js",
  "source": "src/index.tsx",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogbender/fogbender-oss/blob/main/packages/fogbender-react"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsup --dts",
    "start": "microbundle watch --jsx React.createElement --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:lint",
    "test:lint": "eslint . || true",
    "predeploy": "cd example && yarn install && yarn run build"
  },
  "dependencies": {
    "fogbender": ">=0.6.0"
  },
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/user-event": "^10.1.0",
    "@types/jest": "^25.1.4",
    "@types/node": "13.13.2",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
  },
  "files": [
    "dist"
  ]
}
