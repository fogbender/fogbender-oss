{
  "name": "fogbender",
  "version": "0.4.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Made with create-react-library",
  "author": "JLarky",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogbender/fogbender-oss/blob/main/packages/fogbender"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsup",
    "build-faster": "tsup --clean false",
    "start": "run-p start:tsup start:tsc",
    "start:tsup": "tsup --watch",
    "start:tsc": "tsc --noEmit --watch | awk '{gsub(/\\033c/,\"\") system(\"\")}1'",
    "prepare": "run-s build",
    "test": "run-s test:tsc test:lint",
    "test:lint": "eslint .",
    "test:tsc": "tsc --noEmit",
    "predeploy": "cd example && yarn install && yarn run build"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@eslint/json": "^0.10.0",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/user-event": "^10.1.0",
    "@types/jest": "^25.1.4",
    "@types/node": "^22.13.8",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "cross-env": "^7.0.2",
    "css-element-queries": "^1.2.3",
    "esbuild-plugin-solid": "^0.4.2",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "tsup": "^8.0.1",
    "tsup-preset-solid": "^2.1.0",
    "typescript": "^5.8.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@babel/eslint-parser": "^7.26.8",
    "solid-js": "1.8.0",
    "twind": "^0.16.19"
  },
  "browser": {},
  "typesVersions": {}
}
