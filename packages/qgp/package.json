{
  "name": "qgp",
  "version": "0.0.12",
  "publishConfig": {
    "access": "public"
  },
  "description": "Quark-gluon plasma is the hottest frameworks in Astrophysics",
  "author": "JLarky",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fogbender/fogbender-oss/blob/main/packages/qgp"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "source": "src/index.ts",
  "bin": {
    "qgp": "dist/bin.js"
  },
  "files": [
    "templates",
    "dist"
  ],
  "scripts": {
    "build": "tsup-node src/bin.ts src/index.ts --format cjs,esm --dts --clean",
    "watch": "yarn build --watch src",
    "dev": "vitest dev",
    "lint": "eslint || true",
    "test": "yarn lint && vitest run --coverage"
  },
  "dependencies": {
    "pathe": "^1.0.0",
    "vite": "^6.2.0",
    "vite-plugin-env-compatible": "^2.0.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "astro": "*"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@vitest/coverage-v8": "3.0.7",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "ora": "^8.2.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7",
    "which-pm-runs": "^1.1.0"
  }
}
