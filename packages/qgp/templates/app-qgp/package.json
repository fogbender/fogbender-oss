{
  "name": "@example/qgp",
  "description": "Example of Astro app with React SPA and vite-powered HMR",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "start-qgp": "vite --config qgp.config.mjs"
  },
  "dependencies": {
    "astro": "^1.9.0",
    "@astrojs/react": "^1.2.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "qgp": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "*",
    "typescript": "*",
    "vite-plugin-checker": "*"
  },
  "__qgp__": {
    "type": "app",
    "displayName": "Astro+Vite React SPA",
    "nextSteps": {
      "title": "What's next?",
      "lines": [
        "Astro:",
        "  - `npm run dev` and http://localhost:3000/astro",
        "  - `npm run build` and `npm run preview`",
        "  - keep in mind production build is stored inside `dist` folder instead of `build`",
        "",
        "Vite:",
        "  - `npm run start-qgp` and http://localhost:5173"
      ]
    },
    "priority": 1
  }
}
