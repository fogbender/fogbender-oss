---
import Layout from "../layouts/SPA-layout.astro";
import { App } from "../qgp/react";
import { getVersion } from "./ssg/version.json";
const version = getVersion();
---

<Layout title="Fogbender | Team messaging widget for B2B support | B2B support platform">
  <script data-version={version}>
    window._fog_version = document.currentScript?.dataset.version;
  </script>
  <App client:only="react" />
</Layout>
