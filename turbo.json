{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "storefront#build": {
      "dependsOn": ["^build"],
      "inputs": ["*.*", "{src,public}/**", "../client/src/shared"]
    },
    "vendor-demo#build": {
      "dependsOn": ["^build"],
      "inputs": ["*.*", "{src,public}/**", "../client/src/shared/ui/SelectSearch.tsx"]
    },
    "lint:full": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "start": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "preview": {
      "dependsOn": ["build"],
      "cache": false,
      "persistent": true
    }
  }
}
